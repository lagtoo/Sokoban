int main()
{
    //rows columns
    char *level1[11][22] = {{'.','.','.','.','#','#','#','#','#','.','.','.','.','.','.','.','.','.','.','.','.','.'},{'.','.','.','.','#','.','.','.','#','.','.','.','.','.','.','.','.','.','.','.','.','.'},{'.','.','.','.','#','$','.','.','#','.','.','.','.','.','.','.','.','.','.','.','.','.'},{'.','.','#','#','#','.','.','$','#','#','#','.','.','.','.','.','.','.','.','.','.','.'},{'.','.','#','.','.','$','.','.','$','.','#','.','.','.','.','.','.','.','.','.','.','.'},{'#','#','#','.','#','.','#','#','#','.','#','.','.','.','.','.','#','#','#','#','#','#'},{'#','.','.','.','#','.','#','#','#','.','#','#','#','#','#','#','#','.','.','O','O','#'}};
/*#.$..$.............OO#
#####.####.#@####..OO#
....#......###..######
....########..........*/

    int i,j;
    
    for(i=0;i<7;i++)
    {
        for(j=0;i<22;j++)
        {
            printf("%c",level1[i][j]);
            printf("\n");
        }
    }
    
    
    
    
    return 0;
}
